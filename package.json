{
  "name": "demoman",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "test:rust": "cargo test",
    "test": "npm run test:rust",
    "lint:rust": "cargo clippy",
    "lint:js": "eslint . --cache",
    "lint:css": "stylelint --cache src/**/*.css",
