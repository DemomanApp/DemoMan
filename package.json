    "@tauri-apps/api": "^1.5.1",
    "memoize-one": "^6.0.0",
    "nanoid": "^5.0.2",
    "react": "^18.2.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.7",
    "tauri-plugin-log-api": "github:tauri-apps/tauri-plugin-log#v1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.6",
    "@types/node": "^20.8.0",
